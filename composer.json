{
    "name" : "pageon/stitcher-core",
    "description" : "Static website generator in PHP",
    "prefer-stable" : true,
    "require" : {
        "symfony/yaml" : "^4.0",
        "erusev/parsedown" : "^1.6",
        "symfony/filesystem" : "^4.0",
        "twig/twig" : "^2.4",
        "intervention/image" : "^2.4",
        "vlucas/phpdotenv" : "^2.4",
        "symfony/finder": "^4.0",
        "illuminate/support": "^5.5",
        "symfony/routing": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0",
        "leafo/scssphp": "^0.7.1",
        "nikic/fast-route": "^1.2",
        "mrclay/minify": "^3.0",
        "spatie/image-optimizer": "^1.1",
        "league/commonmark": "^0.18.1"
    },
    "require-dev" : {
        "larapack/dd" : "^1.0",
        "phpunit/phpunit" : "^6.0",
        "symfony/process": "^4.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload" : {
        "psr-4" : {
            "Stitcher\\" : "src/Stitcher",
            "Pageon\\" : "src/Pageon"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Stitcher\\Test\\" : "tests",
            "Stitcher\\Test\\Plugin\\" : "tests/test_project/src/Plugin",
            "Stitcher\\Test\\Controller\\" : "tests/test_project/src/Controller",
            "Pageon\\Test\\" : "tests"
        },
        "files": [
            "src/Pageon/helpers.php"
        ]
    }
}
